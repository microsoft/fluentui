{
  "extends": "../../tsconfig.base.v8.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "lib",
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "declaration": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "noUnusedLocals": true,
    "preserveConstEnums": true,
    "lib": ["es5", "dom", "es2015.promise"],
    "types": ["webpack-env", "custom-global"]
  },
  "include": ["src"]
}
