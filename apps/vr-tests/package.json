    "@fluentui/react-theme-provider": "^0.19.16",
    "@uifabric/example-data": "^7.1.6",
    "@uifabric/experiments": "^7.45.26",
    "@uifabric/react-cards": "^0.119.27",
    "@uifabric/react-hooks": "^7.16.4",
    "@uifabric/set-version": "^7.0.24",
    "office-ui-fabric-react": "^7.204.0",
    "react": "16.8.6",
    "react-app-polyfill": "~1.0.1",
    "react-dom": "16.8.6",
    "tslib": "^1.10.0",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@fluentui/eslint-plugin": "^0.54.2",
    "@storybook/addon-options": "^5.3.8",
    "@storybook/addons": "^5.3.8",
    "@storybook/channels": "^5.3.8",
    "@storybook/react": "^5.3.8",
    "@types/react": "16.8.25",
    "@types/react-dom": "16.8.4",
    "@types/webpack-env": "1.15.1",
    "@uifabric/build": "^7.0.0",
    "@uifabric/date-time": "^7.23.33",
    "awesome-typescript-loader": "^3.2.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.5.3",
    "csstype": "^2.6.7",
    "file-loader": "^0.11.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "storybook-readme": "^3.3.0",
    "storywright": "0.0.26-beta.1",
    "style-loader": "^1.2.1",
    "vrscreenshotdiff": "0.0.16"
  }
