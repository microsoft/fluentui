{
  "name": "xplat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native rnx-start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "macos": "react-native run-macos --scheme xplat",
    "windows": "react-native xplat-run-windows",
    "build:android": "react-native rnx-bundle --platform android --dev true",
    "build:ios": "react-native rnx-bundle --platform ios --dev true",
    "build:macos": "react-native rnx-bundle --platform macos --dev true",
    "build:windows": "react-native rnx-bundle --platform windows --dev true",
    "check-deps": "rnx-align-deps",
    "fix-deps": "rnx-align-deps --write",
    "xplat:test": "jest",
    "xplat:lint": "eslint ."
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.73.0",
    "react-native-macos": "^0.73.0",
    "react-native-windows": "^0.73.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.0",
    "@react-native/metro-config": "^0.73.0",
    "@rnx-kit/align-deps": "^2.2.4",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.5",
    "@rnx-kit/cli": "^0.16.17",
    "@rnx-kit/metro-config": "^1.3.14",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.33",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.23.0",
    "jest": "^29.2.1",
    "prettier": "^2.4.1",
    "react-native-test-app": "^3.2.16",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.73"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "core-macos",
        "core-windows",
        "core/metro-config",
        "jest",
        "react",
        "react-test-renderer"
      ]
    },
    "bundle": {
      "entryFile": "index.js",
      "targets": [
        "android",
        "ios",
        "macos",
        "windows"
      ],
      "platforms": {
        "android": {
          "bundleOutput": "dist/main.android.jsbundle",
          "sourcemapOutput": "dist/main.android.bundle.map",
          "assetsDest": "dist/res"
        },
        "ios": {
          "bundleOutput": "dist/main.ios.jsbundle",
          "sourcemapOutput": "dist/main.ios.jsbundle.map",
          "assetsDest": "dist"
        },
        "macos": {
          "bundleOutput": "dist/main.macos.jsbundle",
          "sourcemapOutput": "dist/main.macos.jsbundle.map",
          "assetsDest": "dist"
        },
        "windows": {
          "bundleOutput": "dist/main.windows.bundle",
          "sourcemapOutput": "dist/main.windows.bundle.map",
          "assetsDest": "dist"
        }
      }
    }
  }
}
