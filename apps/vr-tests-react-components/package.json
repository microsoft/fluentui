    "@fluentui/react-select": "9.0.0-beta.22",
    "@fluentui/react-shared-contexts": "^9.1.5",
    "@fluentui/react-slider": "^9.0.17",
    "@fluentui/react-spinner": "^9.0.17",
    "@fluentui/react-spinbutton": "^9.0.15",
