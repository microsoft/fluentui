    "@fluentui/scripts": "^1.0.0",
    "@griffel/react": "^1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "screener-storybook": "0.23.0",
    "tslib": "^2.1.0"
  }
