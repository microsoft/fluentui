    "@fluentui/react-tooltip": "^9.1.16",
    "@fluentui/react-toolbar": "^9.0.6",
    "@fluentui/react-utilities": "^9.5.3",
    "@griffel/react": "^1.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "^2.1.0"
