    "@fluentui/react-table": "9.0.0-alpha.10",
    "@fluentui/react-text": "^9.1.5",
    "@fluentui/react-textarea": "^9.1.4",
    "@fluentui/react-theme": "^9.1.1",
    "@fluentui/react-tooltip": "^9.1.0",
    "@fluentui/react-utilities": "^9.2.0",
    "@fluentui/scripts": "^1.0.0",
    "@griffel/react": "^1.4.2",
    "react": "17.0.2",
