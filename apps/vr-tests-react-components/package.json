{
  "name": "@fluentui/vr-tests-react-components",
  "version": "9.0.0-rc.0",
  "private": true,
  "description": "Visual regression tests for @fluentui/react-components",
  "scripts": {
    "build": "build-storybook -o dist/storybook",
    "clean": "just-scripts clean",
    "format": "prettier . -w --ignore-path ../../.prettierignore",
    "lint": "just-scripts lint",
    "start": "start-storybook",
    "test": "just-scripts test",
    "type-check": "tsc",
    "vr:build": "yarn build",
    "vr:test": "storywright  --browsers chromium --url dist/storybook --destpath dist/screenshots --waitTimeScreenshot 500 --concurrency 4 --headless true"
  },
  "devDependencies": {
    "@fluentui/eslint-plugin": "*",
    "@fluentui/scripts-tasks": "*"
  },
  "dependencies": {
    "@fluentui/react-accordion": "^9.0.18",
    "@fluentui/react-avatar": "^9.2.13",
    "@fluentui/react-badge": "^9.0.19",
    "@fluentui/react-button": "^9.1.15",
    "@fluentui/react-card": "9.0.0-beta.39",
    "@fluentui/react-checkbox": "^9.0.20",
    "@fluentui/react-combobox": "^9.0.0-beta.23",
    "@fluentui/react-dialog": "^9.1.8",
    "@fluentui/react-divider": "^9.1.11",
    "@fluentui/react-field": "9.0.0-alpha.15",
    "@fluentui/react-icons": "^2.0.175",
    "@fluentui/react-image": "^9.0.18",
    "@fluentui/react-infobutton": "9.0.0-beta.7",
    "@fluentui/react-input": "^9.2.12",
    "@fluentui/react-label": "^9.0.17",
    "@fluentui/react-link": "^9.0.18",
    "@fluentui/react-menu": "^9.6.2",
    "@fluentui/react-persona": "^9.1.2",
    "@fluentui/react-popover": "^9.4.1",
    "@fluentui/react-positioning": "^9.3.8",
    "@fluentui/react-progress": "9.0.0-alpha.12",
    "@fluentui/react-provider": "^9.2.2",
