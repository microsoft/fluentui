    "@fluentui/react-theme": "^9.1.5",
    "@fluentui/react-tooltip": "^9.1.8",
    "@fluentui/react-toolbar": "9.0.0-rc.5",
    "@fluentui/react-utilities": "^9.4.0",
    "@griffel/react": "^1.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "^2.1.0"
  }
