      ]
    },
    "e2e:integration": {
      "dependsOn": [],
      "inputs": ["{projectRoot}/cypress-react-18.config.ts", "{workspaceRoot}/packages/react-examples/**/*.e2e.tsx?"]
    }
  }
}
