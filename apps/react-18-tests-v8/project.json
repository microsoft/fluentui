{
  "name": "react-18-tests-v8",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "tags": ["v8"],
  "implicitDependencies": [],
  "targets": {
    "test": {
      "dependsOn": ["^build"]
    },
    "type-check:integration": {
      "dependsOn": ["^build"],
      "inputs": [
        "{projectRoot}/tsconfig.react-18.json",
