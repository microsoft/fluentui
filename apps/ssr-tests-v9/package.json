    "test:ssr": "ts-node --swc ./src/test.ts",
    "test:unit": "jest",
    "type-check": "tsc -b tsconfig.json"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.15.1"
  },
  "devDependencies": {
    "@fluentui/eslint-plugin": "*",
    "@fluentui/scripts-puppeteer": "*",
    "@fluentui/scripts-storybook": "*",
    "@fluentui/scripts-tasks": "*"
  }
}
