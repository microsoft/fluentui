{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "recipe-generator",
  "type": "object",
  "properties": {
    "recipeName": {
      "type": "string",
      "description": "Recipe name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Please enter the recipe name"
    }
  },
  "required": ["recipeName"]
}
