{
  "presets": [
    [
      "@griffel",
      {
        "modules": [
          { "moduleSource": "@griffel/core", "importName": "makeStyles" },
          { "moduleSource": "@griffel/react", "importName": "makeStyles" }
        ]
      }
    ]
  ],
  "plugins": ["annotate-pure-calls", "@babel/transform-react-pure-annotations"]
}
