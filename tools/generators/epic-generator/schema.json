{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "epic-generator",
  "type": "object",
  "properties": {
    "repository": {
      "type": "string",
      "description": "Repository to create issues at."
    },
    "title": {
      "type": "string",
      "description": "Title used in the creation of the epic"
    },
    "message": {
      "type": "string",
      "description": "Message used in the body of the epic"
    }
  },
  "required": ["title"]
}
