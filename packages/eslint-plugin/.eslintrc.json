{
  // eslint config for the package itself
  "extends": ["plugin:@fluentui/eslint-plugin/node"],
  "root": true,
  "rules": {
    // too many conventions requiring quoted identifier types
    "@typescript-eslint/naming-convention": [
      "error",
      {
        "selector": "property",
        "modifiers": ["requiresQuotes"],
        "format": null
      }
    ]
  }
}
