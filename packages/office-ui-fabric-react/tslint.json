{
  "extends": [
    "office-ui-fabric-react-tslint"
  ],
  "rules": {
    "jsx-no-string-ref": false,
    "jsx-key": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false,
    "jsx-no-lambda": false,
    "jsx-ban-props": false,
    "max-line-length": [
      false,
      140
    ],
    "no-any": false,
    "variable-name": false,
    "no-internal-module": false,
    "prefer-const": false,
    "typedef": [
      false
    ],
    "no-inferrable-types": [
      false
    ]
  }
}