{
  "extends": [
    "office-ui-fabric-react-tslint"
  ],
  "rules": {
    "max-line-length": [
      false,
      140
    ],
    "no-any": false,
    "variable-name": false,
    "no-internal-module": false,
    "prefer-const": false,
    "typedef": [
      false
    ],
    "no-inferrable-types": [
      false
    ],
    "no-unused-variable": false
  }
}