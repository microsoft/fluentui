  "files": [
    "*.md",
    "dist/*.d.ts",
    "lib",
    "lib-commonjs",
    "unstable"
  ],
  "wyw-in-js": {
    "tags": {
      "makeStyles": "@griffel/tag-processor/make-styles",
      "makeResetStyles": "@griffel/tag-processor/make-reset-styles"
    }
  }
