{
  "$schema": "https://json.schemastore.org/swcrc",
  "env": {
    "targets": {
      "chrome": "84",
      "edge": "84",
      "firefox": "75",
      "opera": "73",
      "safari": "14.1"
    },
    "bugfixes": true
  },
  "exclude": [
    "/testing",
    "/**/*.cy.ts",
    "/**/*.cy.tsx",
    "/**/*.spec.ts",
    "/**/*.spec.tsx",
    "/**/*.test.ts",
    "/**/*.test.tsx"
  ],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": false
    },
    "externalHelpers": true,
    "transform": {
      "react": {
        "runtime": "classic",
        "useSpread": true
      }
    }
  },
  "minify": false,
  "sourceMaps": true
}
