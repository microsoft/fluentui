{
  "presets": [
    [
      "@griffel",
      {
        "babelOptions": {
          "plugins": [
            [
              "babel-plugin-module-resolver",
              {
                "root": ["../../../../"],
                "alias": {
                  "@fluentui/tokens": "packages/tokens/lib/index.js",
                  "^@fluentui/(?!react-icons)(.+)": "packages/react-components/\\1/library/lib/index.js"
                }
              }
            ]
          ]
        },
        "modules": [
          {
            "moduleSource": "@griffel/core",
            "importName": "makeStyles"
          }
        ]
      }
    ]
  ],
  "plugins": ["annotate-pure-calls", "@babel/transform-react-pure-annotations"]
}
