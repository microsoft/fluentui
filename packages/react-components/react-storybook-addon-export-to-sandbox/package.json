{
  "name": "@fluentui/react-storybook-addon-export-to-sandbox",
  "version": "0.1.0",
  "description": "Storybook addon that enables cloud/web sandbox exports for stories doc mode code examples",
  "private": true,
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui"
  },
  "license": "MIT",
  "devDependencies": {
    "@fluentui/eslint-plugin": "*",
    "@fluentui/scripts-api-extractor": "*"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.1",
    "babel-loader": "9.1.3",
    "codesandbox-import-utils": "2.2.3",
    "@fluentui/babel-preset-storybook-full-source": "^0.0.1",
    "dedent": "^1.2.0"
  },
  "peerDependencies": {
    "@storybook/manager-api": "^7.6.20",
    "@storybook/preview-api": "^7.6.20",
    "@storybook/react": "^7.6.20"
  },
  "beachball": {
    "disallowedChangeTypes": [
      "major",
      "prerelease"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./lib/index.js",
      "node": "./lib-commonjs/index.js",
      "require": "./lib-commonjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "*.md",
    "dist/*.d.ts",
    "lib",
    "lib-commonjs",
    "preset.js"
  ]
}
