{
  "extends": "../../../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noEmit": true,
    "isolatedModules": true,
    "importHelpers": true,
    "jsx": "react",
    "noUnusedLocals": false, // ToDo - remove this line after fixing all unused variables
    "preserveConstEnums": true,
