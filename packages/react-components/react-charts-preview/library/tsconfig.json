    "strictBindCallApply": false,
    "strictFunctionTypes": false,
    "strictNullChecks": true,
    "noImplicitReturns": false,
    "noEmitOnError": false,
    "lib": ["es5", "dom"]
  },
  "include": [],
  "files": [],
  "references": [
    {
      "path": "./tsconfig.lib.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
