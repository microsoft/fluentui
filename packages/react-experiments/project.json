{
  "name": "react-experiments",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/react-experiments/src",
  "tags": ["v8", "ships-bundle"],
  "implicitDependencies": [],
  "metadata": {
    "targetGroups": {
      "React Integration Tester": [
        "test-rit--17--prepare",
        "test-rit--17--test",
        "test-rit--19--prepare",
        "test-rit--19--test",
        "test-rit"
      ]
    }
  },
  "targets": {
    "test": {
      "dependsOn": ["^build"]
    },

    "test-rit": {
      "executor": "nx:noop",
      "cache": true,
      "dependsOn": [
        {
          "target": "test-rit--17--test",
          "projects": "self",
          "params": "forward"
        },
        {
          "target": "test-rit--19--test",
          "projects": "self",
          "params": "forward"
        }
      ],
      "inputs": [
        "default",
        "production",
        "^production",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/tools/react-integration-testing/**"
      ],
      "outputs": [],
      "metadata": {
        "technologies": ["react-integration-tester"],
        "description": "Run react integration tests against React 17, 19",
        "help": {
          "command": "yarn rit --help",
          "example": {}
        }
      },
      "configurations": {},
      "options": {},
      "parallelism": true
    },
    "test-rit--17--prepare": {
      "options": {
        "cwd": "{projectRoot}",
        "command": "yarn rit --prepare-only --no-install --project-id ci --react 17 --verbose"
      },
      "cache": true,
      "inputs": [
        "default",
        "production",
        "^production",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/tools/react-integration-testing/**"
      ],
      "outputs": ["{workspaceRoot}/tmp/rit/react-17/react-experiments-ci"],
      "dependsOn": ["^build"],
      "metadata": {
        "technologies": ["react-integration-tester"],
        "description": "Run react integration tests against React 17",
        "help": {
          "command": "yarn rit --help",
          "example": {}
        }
      },
      "executor": "nx:run-commands",
      "configurations": {},
      "parallelism": true
    },
    "test-rit--17--test": {
      "options": {
        "cwd": "{projectRoot}",
        "command": "yarn rit --project-id ci --react 17 --run test --verbose"
      },
      "cache": true,
      "inputs": [
        "default",
        "production",
        "^production",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/tools/react-integration-testing/**"
      ],
      "outputs": [],
      "dependsOn": ["test-rit--17--prepare"],
      "metadata": {
        "technologies": ["react-integration-tester"],
        "description": "Run react integration tests against React 17",
        "help": {
          "command": "yarn rit --help",
          "example": {}
        }
      },
      "executor": "nx:run-commands",
      "configurations": {},
      "parallelism": true
    },
    "test-rit--19--prepare": {
      "options": {
        "cwd": "{projectRoot}",
        "command": "yarn rit --prepare-only --no-install --project-id ci --react 19 --verbose"
      },
      "cache": true,
      "inputs": [
        "default",
        "production",
        "^production",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/tools/react-integration-testing/**"
      ],
      "outputs": ["{workspaceRoot}/tmp/rit/react-19/app/react-experiments-ci"],
      "dependsOn": ["^build"],
      "metadata": {
        "technologies": ["react-integration-tester"],
        "description": "Run react integration tests against React 19",
        "help": {
          "command": "yarn rit --help",
          "example": {}
        }
      },
      "executor": "nx:run-commands",
      "configurations": {},
      "parallelism": true
    },
    "test-rit--19--test": {
      "options": {
        "cwd": "{projectRoot}",
        "command": "yarn rit --project-id ci --react 19 --run test --verbose"
      },
      "cache": true,
      "inputs": [
        "default",
        "production",
        "^production",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/tools/react-integration-testing/**"
      ],
      "outputs": [],
      "dependsOn": ["test-rit--19--prepare"],
      "metadata": {
        "technologies": ["react-integration-tester"],
        "description": "Run react integration tests against React 19",
        "help": {
          "command": "yarn rit --help",
          "example": {}
        }
      },
      "executor": "nx:run-commands",
      "configurations": {},
      "parallelism": true
    }
  }
}
