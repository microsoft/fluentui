        },
        {
          "ref": "$checked-custom",
          "optional": true
        }
      ],
      "tags": {
        "<div>, <span>, <a>": {
          "parameters": {
            "@tabbable": {
              "ref": "$tabbable",
              "optional": true
            }
          },
          "attributes": [
            {
              "name": "role",
              "value": "treeitem",
              "overridable": "role"
            },
            {
              "ref": "$tabindex"
            }
          ]
        }
      }
    },
    "TreeItemList": {
