{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["es2015", "dom"],
    "strict": true,
    "jsx": "react",
    "rootDir": "src",
    "outDir": "lib",
    "sourceMap": true,
    "types": ["node", "jest", "expect-puppeteer", "jest-environment-puppeteer", "webpack-env"],
    "noUnusedLocals": true
  },
  "include": ["src"],
  "references": []
}
