{
  "extends": ["plugin:@fluentui/eslint-plugin/react"],
  "root": true,
  "overrides": [
    {
      "files": ["src/*.ts"],
      "rules": {
        "@typescript-eslint/naming-convention": [
          "error",
          {
            "selector": "property",
            "modifiers": ["requiresQuotes"],
            "format": null
          }
        ]
      }
    }
  ]
}
