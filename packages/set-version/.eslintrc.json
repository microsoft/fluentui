{
  "extends": ["plugin:@fluentui/eslint-plugin/react--legacy"],
  "root": true,
  "rules": {
    "prefer-const": "off",
    "no-restricted-globals": "off"
  },
  "overrides": [
    {
      "files": ["**/*.{ts,tsx}"],
      "rules": {
        "@typescript-eslint/no-deprecated": "off"
      }
    }
  ]
}
